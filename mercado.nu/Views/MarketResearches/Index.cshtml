@model IEnumerable<mercado.nu.Models.MarketResearch>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Skapa marknadsundersökning</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Purpose)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreationDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StartDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Purpose)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreationDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndDate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MarketResearchId">Uppdatera</a> |
                <a asp-action="Details" asp-route-id="@item.MarketResearchId">Detaljer</a> |
                <a asp-action="Delete" asp-route-id="@item.MarketResearchId">Ta bort undersökning</a> |
                <a asp-controller="Questions" asp-action="ShowQuestionsForMarketResearch" asp-route-marketResearchId="@item.MarketResearchId">Visa frågor</a> |
                <a asp-controller="MarketResearches" asp-action="CreateQuestionsToMarketResearch" asp-route-id="@item.MarketResearchId">Lägg till frågor</a> |
                <a asp-controller="Answers" asp-action="Evaluation" asp-route-marketResearchId="@item.MarketResearchId">Utvärdering av undersökning</a> |
            </td>
        </tr>
}
    </tbody>
</table>
